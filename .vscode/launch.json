    "version": "0.2.0",
    "configurations": [

        {
            "name": "Django server",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver"
            ],
            "django": true,
            "justMyCode": true
        },
        {
            "name": "Python: analytics_update",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "analytics_update", "-v3"
            ],
            "django": true,
            "justMyCode": true
        },
        {
            "name": "Python: documents_check",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "documents_check"
            ],
            "django": true,
            "justMyCode": true
        },
        {
            "name": "Django: reset_db",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "reset_db"
            ],
            "django": true,
            "justMyCode": false
        },
        {
            "name": "Django: migrate",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "migrate"
            ],
            "django": true,
            "justMyCode": true
        },
    ]
